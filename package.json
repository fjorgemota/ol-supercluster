  "devDependencies": {
    "@types/ol": "^5.3.5",
    "@types/supercluster": "^5.0.1",
    "microbundle": "^0.11.0",
    "mocha": "^6.2.0"
  },
  "peerDependencies": {
    "ol": "^5.3.3"
  },
